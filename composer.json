{
    "name": "kinetechsolutions/vordpress",
    "description": "vord.press - Virtualized WordPress",
    "authors": [
        {
            "name": "Aaron Croissette",
            "email": "vordpress@kinetech.llc"
        }
    ],
    "repositories":[
        {
            "type": "composer",
            "url": "https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "require": {},
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "wp-content/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        }
    }
}
